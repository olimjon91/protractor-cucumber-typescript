  "main": "index.js",
  "scripts": {
    "cross-env": "cross-env",
    "build": "tsc",
    "webdriver-manager": "webdriver-manager",
    "clean": "rm -rf typeScript/",
    "clean-build": "npm run clean && npm run build",
    "pretest": "npm run clean-build",
    "webdriver-start": "webdriver-manager start",
    "webdriver-update": "webdriver-manager update",
    "webdriver-shutdown": "webdriver-manager shutdown",
    "test": "protractor typeScript/config/protractorRemoteConf.js",
    "test:e2e": "npm-run-all -c webdriver-update test",
    "webdriver-update-ci": "webdriver-manager update --versions.chrome 75.0.3770.140 --standalone --gecko false",
    "test:ci": "npm run test -- --cucumberOpts.tags=\"@regression\" ----params.environment=qual"
  },
  "devDependencies": {
    "@types/cucumber": "^2.0.4",
    "@types/lodash": "^4.14.85",
    "@types/node": "^14.11.10",
