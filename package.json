    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "cucumber": "^3.2.1",
    "cucumber-html-reporter": "^3.0.4",
    "jsonpath-plus": "^0.18.1",
    "mkdirp": "^0.5.1",
    "npm-run": "^4.1.2",
    "properties-reader": "^0.0.16",
    "protractor": "^5.4.3",
    "protractor-cucumber-framework": "^4.0.8",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.8.1",
    "ts-node": "^3.3.0",
    "typescript": "^3.1.6"
