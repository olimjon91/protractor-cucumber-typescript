    "npm-run-all": "^4.1.2",
    "protractor-helper": "^3.7.8",
    "protractor-simple-cucumber-html-reporter-plugin": "^1.0.0",
    "request": "^2.88.2",
    "tsc": "^1.20150623.0",
    "webdriver-manager": "^12.1.7",
    "write-yaml": "^1.0.0",
    "yawn-yaml": "^1.5.0"
  }
